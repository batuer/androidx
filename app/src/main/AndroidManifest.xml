<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:dist="http://schemas.android.com/apk/distribution"
    package="com.gusi.androidx">

    <dist:module dist:instant="true" />

    <application
        android:name=".app.App"
        android:icon="@mipmap/wx"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".module.theme.ThemeActivity"
            android:label="@string/theme"></activity>
        <activity
            android:name=".module.verctor.VectorActivity"
            android:label="@string/vector" />
        <activity
            android:name=".module.preference.PreferenceFragmentActivity"
            android:label="@string/preference_fragment" />
        <activity
            android:name=".module.preference.PreferenceActivity"
            android:label="@string/preference" />
        <activity
            android:name=".module.constraintlayout.ConstraintlayoutActivity"
            android:label="@string/constraint" />
        <activity android:name=".module.MainListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>